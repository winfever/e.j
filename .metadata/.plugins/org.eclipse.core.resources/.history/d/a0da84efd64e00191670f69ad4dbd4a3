package Test.main;

public class MainClass06 {
	public static void main(String[] args) {
		System.out.println("메인 메소드가 시작되었습니다.");
		// 참조 데이터 type 을 담을수 있는 빈 변수 만들기
		String myName=null; // null 을 대입하면 된다.
		
		boolean isRun=false;
		
		if(isRun) {
			myName="김구라";
		}
		System.out.println("메인 메소드가 정료 됩니다.");
	}
}
